import nltk
from nltk.chat.util import Chat, reflections

pairs = [
    ["hi|hello", ["Hello!", "Hi there!"]],
    ["how are you ?", ["I'm fine.", "Doing good!"]],
    ["what is your name ?", ["I am an AI chatbot."]],
    ["bye", ["Goodbye!", "See you later."]],
]

chatbot = Chat(pairs, reflections)
chatbot.converse()
